//Jumps
if (place_meeting(x,y+1,object2))
{
    jumps = jumpsmax;
}
else
{
    if (jumps == jumpsmax) jumps -=1;
}

if (key_jump) &amp;&amp; (jumps &gt; 0)
{
    jumps -= 1;
    vsp = -jumpspeed;
}else{
    if (key_jump2) &amp;&amp; (jumps &gt; 0)
    {
        jumps -= 1;
        vsp = -jumpspeed;
    }
}


if (vsp &lt; 0) &amp;&amp; (!key_jump_held) vsp = max(vsp,-jumpspeed/1.5)
if (vsp &lt; 0) &amp;&amp; (!key_jump2_held) vsp = max(vsp,-jumpspeed/1.5)

